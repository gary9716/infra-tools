# this is the file located at ~/.aws/config

[sso-session yourapp-sso]
sso_start_url=https://yourapp.awsapps.com/start
sso_region=ap-southeast-1
sso_registration_scopes=sso:account:access
[profile yourapp-staging]
sso_session=yourapp-sso
sso_account_id=123456789012
sso_role_name=AdministratorAccess
output=json
[profile yourapp-prod]
sso_session=yourapp-sso
sso_account_id=123456789012
sso_role_name=AdministratorAccess
output=json
